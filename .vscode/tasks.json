{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "- jix run",
			"type": "shell",
			"command": "jix run -f ${file}",
			"group": {
				"kind": "build"
			}
		},
		{
			"label": "jix install",
			"type": "shell",
			"command": "jix install -f ${file}",
			"group": {
				"kind": "build"
			}
		},
		{
			"label": "jix install --dry-run",
			"type": "shell",
			"command": "jix install --dry-run -f ${file}",
			"group": {
				"kind": "build"
			}
		},
		{
			"label": "jix uninstall",
			"type": "shell",
			"command": "jix uninstall -f ${file}",
			"group": {
				"kind": "build"
			}
		}
	]
}
