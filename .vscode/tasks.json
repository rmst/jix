{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "- nux run",
			"type": "shell",
			"command": "nux run -f ${file}",
			"group": {
				"kind": "build"
			}
		},
		{
			"label": "nux install",
			"type": "shell",
			"command": "nux install ${file}",
			"group": {
				"kind": "build"
			}
		},
		{
			"label": "nux install --dry-run",
			"type": "shell",
			"command": "nux install --dry-run ${file}",
			"group": {
				"kind": "build"
			}
		}
	]
}
